{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@adrian-badilla/ui/landing-page": [
        "libs/adrian-badilla/frontend/landing-page/src/index.ts"
      ],
      "@adrian-badilla/ui/landing-page/routes": [
        "libs/adrian-badilla/frontend/landing-page/src/lib/lib.routes"
      ],
      "@adrian-badilla/ui/shared": [
        "libs/adrian-badilla/frontend/shared/src/index.ts"
      ],
      "@adrian-badilla/ui/shared/*": [
        "libs/adrian-badilla/frontend/shared/src/lib/*"
      ],
      "@shared/*": [
        "libs/adrian-badilla/frontend/shared/*"
      ],
      "@adrian-badilla/auth": [
        "libs/adrian-badilla/frontend/auth/src/index.ts"
      ],
      "@adrian-badilla/auth/routes": [
        "libs/adrian-badilla/frontend/auth/src/lib/lib.routes"
      ],
      "@shared/styles/*": [
        "libs/adrian-badilla/frontend/shared/src/lib/styles/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
